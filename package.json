{
  "name": "toxin-hotel-website",
  "version": "1.0.0",
  "author": "Maksim Fyodorov <m.v.fyodoroff@gmail.com>",
  "private": true,
  "license": "MIT",
  "description": "A hotel website, where you can choose dates to stay and book a room you liked from the proposed",
  "keywords": [
    "Hotel",
    "Website",
    "Booking"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/maksimfyodorov/toxin-hotel-website.git"
  },
  "bugs": "https://github.com/maksimfyodorov/toxin-hotel-website/issues",
  "scripts": {
    "clear": "rimraf ./dist",
    "dev": "webpack --config ./config/webpack.development.config.js",
    "watch": "webpack --watch --config ./config/webpack.development.config.js",
    "build": "npm run clear && webpack --config ./config/webpack.production.config.js",
    "start": "webpack serve --open --config ./config/webpack.development.config.js",
    "lint": "eslint ./src",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "autoprefixer": "^10.2.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.2.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "jquery": "^3.5.1",
    "jquery-ui": "^1.12.1",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "normalize.css": "^8.0.1",
    "postcss-loader": "^4.2.0",
    "postcss-reporter": "^7.0.2",
    "postcss-scss": "^3.0.4",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "resolve-url-loader": "^3.1.2",
    "sass": "^1.32.8",
    "sass-loader": "^10.1.1",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1",
    "webpack-merge": "^5.7.3"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-fsd": "^1.0.1",
    "eslint-plugin-import": "^2.22.1",
    "gh-pages": "^3.1.0",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-standard": "^20.0.0",
    "webpack-dev-server": "^3.11.2"
  }
}
